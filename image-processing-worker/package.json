{
  "name": "image-processing-worker",
  "version": "1.0.0",
  "description": "Worker for processing images in the background",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "rimraf dist && tsc && tsc-alias",
    "dev": "tsx watch src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/ --config eslint.config.js",
    "lint:fix": "eslint src/ --config eslint.config.js --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "fix:all": "npm run format && npm run lint:fix",
    "clean": "rimraf dist coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "4.1.3",
    "bullmq": "5.58.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "compression": "1.8.1",
    "dotenv": "17.2.1",
    "envalid": "8.1.0",
    "ioredis": "5.7.0",
    "mongoose": "8.18.0",
    "multer": "2.0.2",
    "reflect-metadata": "0.2.2",
    "sharp": "0.34.3",
    "slugify": "1.6.6",
    "uuid": "11.1.0",
    "winston": "3.17.0",
    "winston-loki": "6.1.3"
  },
  "devDependencies": {
    "@types/compression": "1.8.1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/multer": "2.0.0",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "eslint": "9.34.0",
    "jest": "30.1.1",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "ts-jest": "29.4.1",
    "tsc-alias": "^1.8.10",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  }
}
